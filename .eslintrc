{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true
  },
  "rules": {
    "angular/controller-as-vm": 0,
    "no-unused-vars": 1,
    "angular/controller-as": 1
  }
}
